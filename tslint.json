{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "*.ebnf"
        ]
    },
    "rules": {
        "quotemark": [
            "single"
        ],
        "trailing-comma": false,
        "curly": false,
        "max-classes-per-file": [
            false
        ],
        "no-console": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
